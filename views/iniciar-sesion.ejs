<main class="contenedor contenedor-formularios">
    <h1><%= nombrePag%></h1>
    <form action="/iniciar-sesion" method="POST" class="default-form">
        <div class="campo">
            <label>Correo</label>
            <input type="email" name="email" placeholder="Escribe tu Correo">
        </div>
        <div class="campo">
            <label>Contraseña</label>
            <input type="password" name="password" placeholder="Escribe tu Contraseña">
        </div>
        <div class="campo enviar">
            <input type="submit" class="btn btn-rosa" value="Iniciar Sesión">
        </div>
    </form>
</main>